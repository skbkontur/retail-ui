[ignore]
.*/testing/react-devtools/.*
.*/fbjs/lib/fetchWithRetries.js.flow
.*/fbjs/lib/PromiseMap.js.flow
.*/fbjs/lib/Deferred.js.flow
.*/fbjs/lib/resolveImmediate.js.flow
.*/node_modules/stylelint/.*
.*/node_modules/radium/src/.*

[include]

[libs]
flow/testing.js

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

munge_underscores=true

suppress_comment=.*\\$FlowIssue.*

module.name_mapper='^.*\.less+$' -> '<PROJECT_ROOT>/flow/CssStub'
module.name_mapper='^.*\.png+$' -> '<PROJECT_ROOT>/flow/StringStub'
