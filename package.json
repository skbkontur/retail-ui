{
  "name": "retail-ui",
  "version": "0.17.12",
  "description": "UI Components",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/retail-ui.git"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "eslint",
      "git add"
    ],
    "*.{css,less}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/skbkontur/retail-ui/issues"
  },
  "scripts": {
    "build": "yarn tsc && node scripts/build",
    "deploy": "gh-pages -d styleguide -r git@github.com:skbkontur/react-ui.git",
    "gemini": "cross-env NODE_ENV=test wait-on http-get://localhost:6060/ && gemini test --reporter vflat",
    "precommit": "lint-staged",
    "predeploy": "npm run styleguide:build",
    "prepush": "npm run test",
    "storybook": "start-storybook -p 6060",
    "storybook:build": "build-storybook",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "license": "MIT",
  "dependencies": {
    "add-event-listener": "0.0.1",
    "brcast": "^3.0.1",
    "classnames": "^2.2.5",
    "create-react-context": "^0.2.1",
    "exenv": "^1.2.2",
    "fbemitter": "^2.1.1",
    "fbjs": "^0.8.16",
    "hoist-non-react-statics": "^2.5.0",
    "invariant": "2.2.4",
    "jss": "^9.8.1",
    "jss-preset-default": "^4.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "normalize-wheel": "^1.0.1",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react-flow-types": "^0.2.0-beta.6",
    "react-input-mask": "1.2.2",
    "theming": "^1.3.0",
    "tslib": "^1.9.0",
    "uuid": "^3.2.1",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@skbkontur/typed-css-modules-loader": "^0.1.4",
    "@storybook/addon-actions": "3.4.0",
    "@storybook/addon-knobs": "3.4.0",
    "@storybook/addons": "3.4.0",
    "@storybook/react": "3.4.0",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.9",
    "@types/fbemitter": "^2.0.32",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^22.2.2",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-input-mask": "^1.2.0",
    "@types/storybook__react": "^3.0.7",
    "@types/warning": "^3.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "css-modules-flow-types-loader": "^1.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.67.1",
    "fs-readdir-recursive": "^1.1.0",
    "gemini": "5.6.2",
    "gemini-sauce": "^1.0.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "22.4.3",
    "json-loader": "^0.5.7",
    "less": "3.0.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.0.4",
    "mockdate": "^2.0.2",
    "output-file-sync": "^2.0.1",
    "prettier": "^1.11.1",
    "react": "^16.3.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-docgen-typescript": "^1.3.0",
    "react-dom": "^16.3.1",
    "react-styleguidist": "7.0.8",
    "react-test-renderer": "16.3.1",
    "serve": "^6.5.4",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.2",
    "ts-loader": "3.5.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1",
    "url-loader": "^1.0.1",
    "wait-on": "^2.1.0",
    "webpack": "^3.11.0",
    "webpack-watch-files-plugin": "^1.0.1"
  },
  "peerDependencies": {
    "react": ">=0.14.9",
    "react-addons-css-transition-group": ">=0.14.8",
    "react-dom": ">=0.14.9"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf||svg|ttf|woff|woff2)$": "<rootDir>/testing/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.js": "<rootDir>/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/test-setup.js"
    ]
  }
}
