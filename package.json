{
  "name": "retail-ui",
  "version": "0.6.10",
  "description": "UI Components",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/retail-ui.git"
  },
  "bugs": {
    "url": "https://github.com/skbkontur/retail-ui/issues"
  },
  "scripts": {
    "test": "jest --no-cache && gemini test --reporter vflat",
    "gemini:update": "gemini update",
    "storybook": "start-storybook -p 6060",
    "build-storybook": "build-storybook",
    "typecheck": "flow",
    "lint": "eslint .",
    "unit-test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "add-event-listener": "0.0.1",
    "classnames": "^2.2.5",
    "exenv": "^1.2.1",
    "fbemitter": "^2.0.2",
    "fbjs": "^0.8.9",
    "invariant": "2.2.2",
    "object-assign": "^4.1.1",
    "react-input-mask": "0.7.3",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-knobs": "1.6.0",
    "@kadira/storybook-addons": "1.6.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-jest": "^18.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "0.10.0",
    "flow-bin": "^0.39.0",
    "gemini": "^4.18.1",
    "gemini-sauce": "1.0.0",
    "jest-cli": "^18.1.0",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "react": "^0.14.8",
    "react-addons-css-transition-group": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.8",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0",
    "react-addons-css-transition-group": "^0.14.0 || ^15.0.0-0",
    "react-addons-test-utils": "^0.14.0 || ^15.0.0-0",
    "react-dom": "^0.14.0 || ^15.0.0-0"
  },
  "jest": {
    "transform": {
      "\\.*": "<rootDir>/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*(test|spec))\\.(ts|tsx|js)$",
    "testPathDirs": [
      "<rootDir>/components",
      "<rootDir>/lib",
      "<rootDir>/scripts",
      "<rootDir>/testing"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/babel-polyfill/lib/index.js"
    ]
  }
}
