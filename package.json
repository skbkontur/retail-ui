{
  "name": "retail-ui",
  "version": "0.15.0",
  "description": "UI Components",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/retail-ui.git"
  },
  "lint-staged": {
    "*.js": ["prettier --single-quote --write", "eslint", "git add"],
    "*.{css,less}": ["prettier --write", "git add"],
    "*.ts": ["prettier --single-quote --write", "git add"]
  },
  "bugs": {
    "url": "https://github.com/skbkontur/retail-ui/issues"
  },
  "scripts": {
    "build": "node scripts/build",
    "build-storybook": "build-storybook",
    "deploy": "gh-pages -d styleguide -r git@github.com:skbkontur/react-ui.git",
    "gemini:update": "gemini update",
    "gemini":
      "wait-on http-get://localhost:6060/ && gemini test --reporter vflat",
    "lint": "eslint . --ignore-pattern 'flow-typed/**/*.js'",
    "precommit": "flow && lint-staged",
    "predeploy": "npm run styleguide:build",
    "prepush": "npm run unit-test",
    "storybook": "start-storybook -p 6060",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test":
      "jest --no-cache && concurrently 'npm run storybook' 'npm run gemini' -k -s first",
    "typecheck": "flow",
    "unit-test": "jest",
    "unit-test:watch": "jest --watch"
  },
  "license": "MIT",
  "dependencies": {
    "add-event-listener": "0.0.1",
    "brcast": "^3.0.1",
    "classnames": "^2.2.5",
    "exenv": "^1.2.2",
    "fbemitter": "^2.1.1",
    "fbjs": "^0.8.16",
    "hoist-non-react-statics": "^2.5.0",
    "invariant": "2.2.2",
    "jss": "^9.8.0",
    "jss-preset-default": "^4.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "normalize-wheel": "^1.0.1",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react-flow-types": "^0.2.0-beta.6",
    "react-input-mask": "1.2.2",
    "theming": "^1.3.0",
    "uuid": "^3.2.1",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.13",
    "@storybook/addon-knobs": "3.3.13",
    "@storybook/addons": "3.3.13",
    "@storybook/react": "3.3.13",
    "@types/react": "^16.0.38",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "7.1.2",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.0",
    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-sort-class-members": "^1.3.0",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.66.0",
    "fs-readdir-recursive": "^1.1.0",
    "gemini": "5.5.0",
    "gemini-sauce": "^1.0.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "22.3.0",
    "json-loader": "^0.5.7",
    "less": "3.0.1",
    "less-loader": "^4.0.5",
    "lint-staged": "^6.1.1",
    "mockdate": "^2.0.2",
    "output-file-sync": "^2.0.1",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-docgen-typescript": "^1.2.3",
    "react-dom": "^16.2.0",
    "react-styleguidist":
      "git+https://github.com/nanot1m/react-styleguidist.git#better-flow-props",
    "react-test-renderer": "16.2.0",
    "style-loader": "^0.20.2",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "typescript": "^2.7.2",
    "url-loader": "^0.6.2",
    "wait-on": "^2.1.0",
    "webpack": "^3.11.0"
  },
  "peerDependencies": {
    "@types/react": "*",
    "react": ">=0.14.9",
    "react-addons-css-transition-group": ">=0.14.8",
    "react-dom": ">=0.14.9"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf||svg|ttf|woff|woff2)$":
        "<rootDir>/testing/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.js": "<rootDir>/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*(test|spec))\\.(js|jsx)$",
    "roots": [
      "<rootDir>/components",
      "<rootDir>/lib",
      "<rootDir>/scripts",
      "<rootDir>/testing"
    ],
    "setupFiles": ["<rootDir>/test-setup.js"]
  }
}
