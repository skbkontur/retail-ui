{
  "name": "react-ui-validations-docs",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run compile-markdown-loader && cross-env NODE_ENV=production webpack -p",
    "compile-markdown-loader": "babel ./loaders/markdown-loader.es6 --out-file ./loaders/markdown-loader.js",
    "start": "npm run compile-markdown-loader && webpack-dev-server -d --history-api-fallback --hot --inline",
    "publish-docs": "npm run clean && git clone git@github.com:skbkontur/react-ui-validations.git dist && cd dist && git checkout gh-pages && cd .. && npm run build && cd dist && git add . && git commit --allow-empty -am \"$TRAVIS_TAG\" && git push git@github.com:skbkontur/react-ui-validations.git gh-pages && cd ..",
    "publish-docs-update": "npm run clean && git clone git@github.com:skbkontur/react-ui-validations.git dist && cd dist && git checkout gh-pages && cd .. && npm run build && cd dist && git add . && git commit --allow-empty -am \"update docs\" && git push git@github.com:skbkontur/react-ui-validations.git gh-pages && cd .."
  },
  "description": "Documentation project for react-ui-validations library",
  "author": "tihonove",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^16.0.31",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^6.1.1",
    "retail-ui": "^0.10.4",
    "styled-components": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "classnames-loader": "^2.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-skbkontur-retail": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "markdown": "^0.5.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.1.3",
    "redux": "^3.7.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
