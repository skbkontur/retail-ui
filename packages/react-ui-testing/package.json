{
  "name": "react-ui-testing",
  "private": true,
  "version": "4.0.0",
  "description": "React selenium testing",
  "main": "react-selenium-testing.js",
  "scripts": {
    "build": "dotnet build --configuration Release",
    "postbuild": "yarn --cwd ./TestPages install",
    "test": "dotnet test ./Tests/Tests.csproj --configuration Release --verbosity detailed --framework netcoreapp3.1",
    "start": "yarn --cwd ./TestPages start",
    "styleguide": "styleguidist server --config ./docs/.styleguide/config/base.config.js",
    "styleguide:build": "styleguidist build --config ./docs/.styleguide/config/base.config.js"
  },
  "devDependencies": {
    "react-styleguidist": "^11.1.6"
  },
  "author": "",
  "license": "ISC"
}
