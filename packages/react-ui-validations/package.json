{
  "name": "@skbkontur/react-ui-validations",
  "version": "2.3.7",
  "description": "Validations for @skbkontur/react-ui",
  "scripts": {
    "prerelease": "yarn build && yarn predeploy",
    "release": "npm publish ./build/react-ui-dist --workspaces=false",
    "release:unstable": "yarn release --tag=unstable",
    "prebuild": "yarn clean",
    "clean": "rimraf dist build",
    "build": "npm-run-all --continue-on-error build:*",
    "build:react-ui": "rollup --config ./rollup.react-ui.config.mjs",
    "predeploy": "yarn bundle-dts && yarn prepare-packages && yarn test:pack",
    "bundle-dts": "node scripts/bundle-dts.js",
    "prepare-packages": "node scripts/prepare-packages.js",
    "deploy": "npm-run-all --continue-on-error deploy:*",
    "deploy:react-ui": "yarn --cwd ./build/react-ui-dist publish --non-interactive",
    "storybook": "cross-env STORYBOOK_REACT_UI_TEST=true storybook dev -p 6060",
    "storybook:build": "cross-env STORYBOOK_REACT_UI_TEST=true storybook build -o .storybook/build --quiet",
    "storybook:docs-build": "cross-env STORYBOOK_REACT_UI_VALIDATIONS_DOCS=true storybook build --docs -c .storybook-docs -o .storybook/build --quiet",
    "storybook:docs": "cross-env STORYBOOK_REACT_UI_VALIDATIONS_DOCS=true storybook dev --docs -p 6062 -c .storybook-docs",
    "storybook:serve": "serve -l 6060 .storybook/build",
    "lint": "npm-run-all --continue-on-error --parallel lint:*",
    "lint:tsc": "tsc --noEmit --diagnostics",
    "lint:eslint": "cross-env TIMING=1 eslint ./ --ignore-pattern \"**/docs/**/*.docs.stories.tsx\"",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -c fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:pack": "tsc -p check-dts.tsconfig.json && vitest run -c=vitest.predeploy.config.ts",
    "creevey": "creevey test -c .creevey/config.mts --storybook-port=6060",
    "creevey:ci": "npx playwright install --with-deps chromium && start-server-and-test storybook:serve http://localhost:6060 creevey",
    "creevey:update": "creevey report --update -c .creevey/config.mts",
    "creevey:ui": "yarn creevey --ui",
    "selenium:test": "dotnet test ./selenium-tests/ValidationTests/ValidationTests.csproj --configuration Release --framework netcoreapp3.1",
    "selenium:ci": "start-server-and-test storybook http://localhost:6060/iframe.html selenium:test"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "author": "exactlynosense <whois.ard@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/skbkontur/retail-ui/tree/master/packages/react-ui-validations",
  "bugs": {
    "url": "https://github.com/skbkontur/retail-ui/issues"
  },
  "files": [
    "index.d.ts",
    "index.js",
    "index.js.map"
  ],
  "peerDependencies": {
    "@skbkontur/react-ui": ">=5 <6",
    "react": ">=16.9 <19",
    "react-dom": ">=16.9 <19"
  },
  "dependencies": {
    "smoothscroll-polyfill": "^0.4.4",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-proposal-decorators": "7.24.6",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@playwright/test": "^1.54.2",
    "@rollup/plugin-commonjs": "25.0.8",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.5",
    "@skbkontur/storybook-addon-live-examples": "1.0.0-beta.1",
    "@storybook/addon-docs": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-links": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-webpack5": "8.6.14",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/styled-components": "^5.1.34",
    "@types/warning": "^3.0.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-search-and-replace": "1.1.1",
    "core-js": "^3.37.1",
    "creevey": "0.10.34",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dts-bundle": "^0.7.3",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^27.0.0",
    "less": "4.2.0",
    "less-loader": "12.2.0",
    "markdown": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "playwright-core": "^1.54.2",
    "raw-loader": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.23.1",
    "react-syntax-highlighter": "15.5.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "selenium-webdriver": "^4.27.0",
    "serve": "^13.0.2",
    "shelljs": "^0.8.3",
    "start-server-and-test": "1.14.0",
    "storybook": "8.6.14",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^4.0.0",
    "styled-components": "^6.1.11",
    "ts-loader": "9.5.1",
    "tslib": "2.6.2",
    "typescript": "4.9.4",
    "vitest": "^3.2.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "5.0.4"
  }
}
