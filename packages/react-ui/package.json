{
  "name": "@skbkontur/react-ui",
  "version": "1.8.1",
  "description": "UI Components",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/retail-ui.git"
  },
  "bugs": {
    "url": "https://github.com/skbkontur/retail-ui/issues"
  },
  "scripts": {
    "prerelease": "yarn clean && node scripts/publish/prepublish && yarn build",
    "clean": "git clean -fdxqe node_modules",
    "prebuild": "run-s storybook:build:default build:types",
    "build:types": "tsc -p ./prod.tsconfig.json --emitDeclarationOnly --diagnostics --outDir build",
    "build": "cross-env BABEL_ENV=production node scripts/build",
    "postrelease": "yarn deploy",
    "predeploy": "rimraf .styleguide/build && yarn run styleguide:build",
    "deploy": "gh-pages -a -d .styleguide/build -r git@github.com:skbkontur/react-ui.git",
    "release": "npm publish ./build",
    "storybook": "start-storybook -p 6060",
    "storybook:flat": "cross-env STORYBOOK_FLAT_UI=true start-storybook -p 6060",
    "storybook:build": "run-s storybook:build:*",
    "storybook:build:default": "cross-env enableReactTesting=true build-storybook -o .storybook/build/default",
    "storybook:build:flat": "cross-env enableReactTesting=true STORYBOOK_FLAT_UI=true build-storybook -o .storybook/build/flat",
    "storybook:test": "run-p storybook:test:*",
    "storybook:test:default": "cross-env enableReactTesting=true start-storybook --ci -p 6060",
    "storybook:test:flat": "cross-env enableReactTesting=true STORYBOOK_FLAT_UI=true start-storybook --ci -p 6061",
    "storybook:serve": "run-p storybook:serve:*",
    "storybook:serve:default": "serve -l 6060 .storybook/build/default",
    "storybook:serve:flat": "serve -l 6061 .storybook/build/flat",
    "styleguide": "styleguidist server --config .styleguide/config/dev.config.js",
    "styleguide:build": "node .styleguide/build",
    "lint": "run-p -c lint:*",
    "lint:eslint": "cross-env TIMING=1 eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:tsc": "tsc --noEmit --diagnostics",
    "lint:stylelint": "stylelint \"./**/*.less\"",
    "fix:eslint": "yarn lint:eslint --fix",
    "test": "cross-env BABEL_ENV=development jest",
    "test:watch": "yarn test --watch",
    "creevey": "cross-env BABEL_ENV=development wait-on -t 300000 http-get://localhost:6060/ http-get://localhost:6061/ && creevey -c .creevey/config.ts",
    "creevey:ui": "yarn creevey --ui",
    "report:eslint": "yarn eslint -f eslint-html-reporter/reporter.js -o reports/eslint/index.html",
    "storybook:test-retreat": "cross-env enableReactTesting=true yarn storybook",
    "creevey:test-retreat": "cross-env BABEL_ENV=development wait-on -t 300000 http-get://localhost:6060/ && creevey -c .creevey/config.ts --ui"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "classnames": "^2.2.6",
    "create-emotion": "^10.0.14",
    "csstype": "2.6.7",
    "eventemitter3": "^4.0.0",
    "invariant": "2.2.4",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "normalize-wheel": "^1.0.1",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react-focus-lock": "^2.2.1",
    "react-input-mask": "2.0.4",
    "react-transition-group": "^4.3.0",
    "shallowequal": "^1.1.0",
    "stylis-plugin-extra-scope": "^0.1.1",
    "tabbable": "^4.0.0",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@babel/register": "^7.8.3",
    "@skbkontur/react-icons": "3.2.1",
    "@storybook/addon-actions": "5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "5.3.13",
    "@storybook/react": "5.3.13",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/invariant": "^2.2.30",
    "@types/jest": "^24.0.23",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.throttle": "^4.1.6",
    "@types/mockdate": "^2.0.0",
    "@types/prop-types": "15.7.3",
    "@types/raf": "^3.4.0",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-input-mask": "2.0.5",
    "@types/react-transition-group": "^4.2.3",
    "@types/selenium-webdriver": "^4.0.5",
    "@types/shallowequal": "^1.1.1",
    "@types/tabbable": "^3.1.0",
    "@types/warning": "^3.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "chalk": "3.0.0",
    "core-js": "^3.4.8",
    "creevey": "0.3.6",
    "cross-env": "^6.0.3",
    "css-loader": "3.4.0",
    "dts-css-modules-loader": "1.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "fs-extra": "^8.0.1",
    "fs-readdir-recursive": "^1.1.0",
    "gh-pages": "^2.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.9.0",
    "jest-teamcity-reporter": "^0.9.0",
    "less": "3.10.3",
    "less-loader": "^5.0.0",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "output-file-sync": "^2.0.1",
    "prettier": "^1.14.2",
    "react": "^16.12.0",
    "react-docgen-typescript": "^1.16.1",
    "react-dom": "^16.12.0",
    "react-styleguidist": "^10.4.1",
    "rimraf": "^3.0.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "semver": "^6.3.0",
    "serve": "^11.2.0",
    "style-loader": "^1.0.1",
    "stylelint": "^12.0.0",
    "stylelint-config-css-modules": "^2.1.0",
    "stylelint-config-standard": "^19.0.0",
    "typescript": "^3.7.3",
    "url-loader": "^3.0.0",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.2"
  },
  "peerDependencies": {
    "react": ">=16.9 <17",
    "react-dom": ">=16.9 <17"
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf||svg|ttf|woff|woff2)$": "<rootDir>/components/__mocks__/fileMock.ts",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(j|t)sx?$": "babel-jest"
    },
    "testRegex": "__tests__(\\\\|/).*(\\.|-)test\\.(j|t)sx?$",
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ]
  }
}
