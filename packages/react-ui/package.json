    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "fs-extra": "^11.2.0",
