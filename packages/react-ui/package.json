    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@babel/register": "^7.24.6",
    "@skbkontur/icons": "1.10.0",
    "@skbkontur/storybook-addon-live-examples": "0.0.17",
