    "serve": "^13.0.2",
    "storybook": "7.6.18",
    "style-loader": "^4.0.0",
    "stylelint": "^16.6.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "4.9.4",
    "upath": "^2.0.1",
    "url-loader": "^4.1.1",
