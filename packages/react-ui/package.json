    "@types/warning": "^3.0.0",
    "axios": "^1.7.2",
    "babel-jest": "29.7.0",
    "babel-loader": "^9.1.3",
    "chalk": "4.1.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.37.1",
    "creevey": "0.9.0-beta.12",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "dts-css-modules-loader": "^2.0.1",
