{
  "name": "@skbkontur/react-ui",
  "version": "4.1.1",
  "description": "UI Components",
  "main": "cjs/index.js",
  "module": "index.js",
  "sideEffects": false,
  "homepage": "https://tech.skbkontur.ru/react-ui",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/retail-ui.git"
  },
  "bugs": {
    "url": "https://github.com/skbkontur/retail-ui/issues"
  },
  "scripts": {
    "prebuild": "run-s clean build:types",
    "build:types": "tsc -p ./prod.tsconfig.json --emitDeclarationOnly --diagnostics --outDir build",
    "build:cjs": "cross-env BABEL_ENV=cjs node scripts/build",
    "build:esm": "node scripts/build",
    "build": "run-p build:esm build:cjs && node scripts/build/postbuild",
    "clean": "git clean -fdxqe node_modules",
    "prerelease": "node scripts/publish/prepublish && yarn build",
    "release": "npm publish ./build",
    "postrelease": "yarn deploy",
    "predeploy": "rimraf .styleguide/deploy && yarn run styleguide:build",
    "deploy": "gh-pages -a -d .styleguide/deploy -r git@github.com:skbkontur/react-ui.git",
    "storybook": "start-storybook -p 6060",
    "storybook:build": "cross-env STORYBOOK_REACT_UI_TEST=true build-storybook -o .storybook/build --quiet",
    "storybook:test": "cross-env STORYBOOK_REACT_UI_TEST=true start-storybook --ci -p 6060",
    "storybook:serve": "serve -l 6060 .storybook/build",
    "styleguide": "styleguidist server --config .styleguide/config/dev.config.js",
    "styleguide:build": "node .styleguide/build",
    "lint": "run-p -c lint:*",
    "lint:eslint": "cross-env TIMING=1 yarn eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:tsc": "tsc --noEmit --diagnostics",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -c fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn prettier --write .",
    "test": "cross-env BABEL_ENV=cjs jest --detectOpenHandles",
    "test:watch": "yarn test --watch",
    "creevey": "wait-on -t 300000 http-get://localhost:6060/ && cross-env BABEL_ENV=cjs creevey -c .creevey/config.js",
    "creevey:update": "cross-env BABEL_ENV=cjs creevey -c .creevey/config.js --update",
    "creevey:ui": "yarn creevey --ui",
    "report:eslint": "yarn eslint -f eslint-html-reporter/reporter.js -o reports/eslint/index.html"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.16.5",
    "@emotion/css": "^11.7.1",
    "eventemitter3": "^4.0.7",
    "invariant": "2.2.4",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "normalize-wheel": "^1.0.1",
    "prop-types": "^15.8.0",
    "raf": "^3.4.1",
    "react-focus-lock": "^2.7.1",
    "react-input-mask": "2.0.4",
    "react-is": "^17.0.2",
    "react-transition-group": "^4.4.2",
    "shallowequal": "^1.1.0",
    "stylis-plugin-extra-scope": "^0.3.0",
    "tabbable": "^5.2.1",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-decorators": "7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "7.16.5",
    "@babel/register": "^7.16.5",
    "@emotion/eslint-plugin": "11.7.0",
    "@skbkontur/react-icons": "5.2.0",
    "@skbkontur/react-props2attrs": "0.1.2",
    "@skbkontur/react-sorge": "0.1.0",
    "@storybook/addon-a11y": "6.3.2",
    "@storybook/addon-essentials": "6.3.2",
    "@storybook/addon-links": "6.3.2",
    "@storybook/react": "6.3.2",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/enzyme": "^3.10.10",
    "@types/invariant": "^2.2.35",
    "@types/jest": "27.0.3",
    "@types/jest-axe": "^3.5.3",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.throttle": "^4.1.6",
    "@types/prop-types": "15.7.4",
    "@types/raf": "^3.4.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-input-mask": "2.0.5",
    "@types/react-is": "^17.0.3",
    "@types/react-transition-group": "^4.4.4",
    "@types/shallowequal": "^1.1.1",
    "@types/warning": "^3.0.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.6",
    "axios": "^0.24.0",
    "babel-jest": "27.4.5",
    "babel-loader": "^8.2.3",
    "chalk": "4.1.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.20.1",
    "creevey": "0.7.35",
    "cross-env": "^7.0.3",
    "css-loader": "5.2.0",
    "dotenv": "^10.0.0",
    "dts-css-modules-loader": "1.2.4",
    "enzyme": "^3.11.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-testing-library": "^5.0.5",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "fs-extra": "^10.0.0",
    "fs-readdir-recursive": "^1.1.0",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "27.4.5",
    "jest-axe": "^5.0.1",
    "jest-teamcity-reporter": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "output-file-sync": "^2.0.1",
    "react": "17.0.2",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-styleguidist": "^11.1.7",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "serve": "^13.0.2",
    "style-loader": "^2.0.0",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "4.5.4",
    "upath": "^2.0.1",
    "url-loader": "^4.1.1",
    "wait-on": "^6.0.0",
    "webpack": "^4.41.2"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9"
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "testRegex": "__tests__(\\\\|/).*(\\.|-)test\\.(j|t)sx?$",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
