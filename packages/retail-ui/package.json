{
  "name": "retail-ui",
  "version": "1.12.0",
  "description": "UI Components",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/retail-ui.git"
  },
  "bugs": {
    "url": "https://github.com/skbkontur/retail-ui/issues"
  },
  "scripts": {
    "prepublishOnly": "yarn clean && node scripts/publish/prepublish && yarn build",
    "clean": "git clean -fdxqe node_modules",
    "build": "yarn tsc -p prod.tsconfig.json && node scripts/build",
    "postpublish": "yarn deploy && npm run publish:react-ui",
    "predeploy": "rimraf .styleguide/build && yarn run styleguide:build",
    "deploy": "gh-pages -a -d .styleguide/build -r git@github.com:skbkontur/react-ui.git",
    "publish:react-ui": "npm publish ./build",
    "storybook": "start-storybook -p 6060",
    "storybook:flat": "cross-env STORYBOOK_FLAT_UI=true start-storybook -p 6060",
    "storybook:build": "run-s storybook:build:*",
    "storybook:build:default": "cross-env enableReactTesting=true build-storybook -o .storybook/build/default",
    "storybook:build:flat": "cross-env enableReactTesting=true STORYBOOK_FLAT_UI=true build-storybook -o .storybook/build/flat",
    "storybook:test": "run-p storybook:test:*",
    "storybook:test:default": "cross-env enableReactTesting=true start-storybook --ci -p 6060",
    "storybook:test:flat": "cross-env enableReactTesting=true STORYBOOK_FLAT_UI=true start-storybook --ci -p 6061",
    "storybook:serve": "run-p storybook:serve:*",
    "storybook:serve:default": "serve -l 6060 .storybook/build/default",
    "storybook:serve:flat": "serve -l 6061 .storybook/build/flat",
    "styleguide": "styleguidist server --config .styleguide/config/dev.config.js",
    "styleguide:build": "node .styleguide/build",
    "lint:flow": "flow check",
    "lint": "run-p -c lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint ./",
    "lint:tslint": "tslint --project tsconfig.json",
    "lint:stylelint": "stylelint \"./**/*.less\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "creevey": "wait-on -t 300000 http-get://localhost:6060/ http-get://localhost:6061/ && creevey -c .creevey/config.ts",
    "creevey:ui": "npm run creevey -- --ui"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "classnames": "^2.2.6",
    "create-emotion": "^10.0.9",
    "create-react-context": "^0.2.2",
    "csstype": "^2.6.1",
    "exenv": "^1.2.2",
    "fbemitter": "^2.1.1",
    "fbjs": "^0.8.17",
    "invariant": "2.2.4",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "normalize-wheel": "^1.0.1",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.2",
    "raf": "^3.4.0",
    "react-focus-lock": "^1.11.2",
    "react-input-mask": "2.0.4",
    "react-transition-group": "^2.4.0",
    "stylis-plugin-extra-scope": "^0.1.1",
    "tabbable": "^3.1.1",
    "tslib": "^1.10.0",
    "uuid": "^3.3.2",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/plugin-transform-typeof-symbol": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.10",
    "@skbkontur/logos": "1.0.0",
    "@skbkontur/react-icons": "^3.1.8",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "5.2.6",
    "@storybook/react": "5.2.6",
    "@types/classnames": "^2.2.5",
    "@types/enzyme": "^3.10.3",
    "@types/exenv": "^1.2.0",
    "@types/fbemitter": "^2.0.32",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^24.0.18",
    "@types/lodash.debounce": "^4.0.3",
    "@types/lodash.isequal": "^4.5.3",
    "@types/lodash.throttle": "^4.1.3",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-input-mask": "2.0.1",
    "@types/react-transition-group": "^2.0.11",
    "@types/selenium-webdriver": "^4.0.5",
    "@types/tabbable": "^3.1.0",
    "@types/uuid": "^3.4.3",
    "@types/warning": "^3.0.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "core-js": "^3.2.1",
    "creevey": "0.2.4-3",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^4.19.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.91.0",
    "flow-typed": "^2.5.1",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "fs-extra": "^8.0.1",
    "fs-readdir-recursive": "^1.1.0",
    "gh-pages": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.9.0",
    "jest-teamcity-reporter": "^0.9.0",
    "json-loader": "^0.5.7",
    "less": "3.7.1",
    "less-loader": "^4.1.0",
    "mkdirp": "^0.5.1",
    "mockdate": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "output-file-sync": "^2.0.1",
    "prettier": "^1.13.7",
    "react": "^16.4.2",
    "react-docgen-typescript": "^1.6.2",
    "react-dom": "^16.4.2",
    "react-styleguidist": "^7.3.11",
    "react-test-renderer": "16.3.1",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "semver": "^5.6.0",
    "serve": "^9.2.0",
    "style-loader": "^1.0.0",
    "stylelint": "^9.9.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "6.2.1",
    "ts-node": "^8.5.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typed-css-modules-loader": "^0.0.18",
    "typescript": "^3.3.3333",
    "url-loader": "^1.0.1",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-watch-files-plugin": "^1.0.2"
  },
  "peerDependencies": {
    "@skbkontur/react-icons": "^3.0.0",
    "react": ">=15 <=16",
    "react-dom": ">=15 <=16"
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf||svg|ttf|woff|woff2)$": "<rootDir>/testing/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ],
    "roots": [
      "components",
      "lib",
      "testing",
      "scripts"
    ]
  }
}
