{
  "name": "retail-ui",
  "version": "0.30.0",
  "description": "UI Components",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/retail-ui.git"
  },
  "bugs": {
    "url": "https://github.com/skbkontur/retail-ui/issues"
  },
  "scripts": {
    "build": "yarn tsc -p prod.tsconfig.json && node scripts/build",
    "deploy": "gh-pages -a -d styleguide -r git@github.com:skbkontur/react-ui.git",
    "predeploy": "rimraf styleguide && yarn run styleguide:build",
    "prepush": "yarn run test",
    "prepublishOnly": "yarn clean && yarn build",
    "postpublish": "yarn deploy",
    "storybook": "start-storybook -p 6060",
    "storybook:build": "build-storybook",
    "storybook:run-serve": "npm run storybook:build && serve storybook-static -p 6660",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build && styleguidist build --config styleguide.prod.config.js",
    "clean": "git clean -fdxqe node_modules",
    "lint": "tsc --noEmit && eslint ./ && tslint --project tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "license": "MIT",
  "dependencies": {
    "@skbkontur/react-icons": "^3.0.0",
    "add-event-listener": "0.0.1",
    "classnames": "^2.2.6",
    "create-react-context": "^0.2.2",
    "exenv": "^1.2.2",
    "fbemitter": "^2.1.1",
    "fbjs": "^0.8.17",
    "invariant": "2.2.4",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "normalize-wheel": "^1.0.1",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.2",
    "raf": "^3.4.0",
    "react-focus-lock": "^1.11.2",
    "react-input-mask": "2.0.4",
    "react-transition-group": "^2.4.0",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2",
    "warning": "^4.0.1"
  },
  "devDependencies": {
    "@skbkontur/typed-css-modules-loader": "^0.1.4",
    "@storybook/addon-actions": "3.4.8",
    "@storybook/addon-knobs": "3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "3.4.8",
    "@storybook/react": "3.4.8",
    "@types/classnames": "^2.2.5",
    "@types/enzyme": "^3.1.12",
    "@types/enzyme-to-json": "^1.5.1",
    "@types/exenv": "^1.2.0",
    "@types/fbemitter": "^2.0.32",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^23.3.0",
    "@types/lodash.debounce": "^4.0.3",
    "@types/lodash.isequal": "^4.5.3",
    "@types/lodash.throttle": "^4.1.3",
    "@types/prop-types": "^15.5.5",
    "@types/react": "^16.4.9",
