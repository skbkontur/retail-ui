{
  "name": "retail-ui",
  "version": "0.51.1",
  "description": "UI Components",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/retail-ui.git"
  },
  "bugs": {
    "url": "https://github.com/skbkontur/retail-ui/issues"
  },
  "scripts": {
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "git clean -fdxqe node_modules",
    "build": "yarn tsc -p prod.tsconfig.json && node scripts/build",
    "postpublish": "yarn deploy && yarn publish:react-ui",
    "predeploy": "rimraf .styleguide/build && yarn run styleguide:build",
    "deploy": "gh-pages -a -d .styleguide/build -r git@github.com:skbkontur/react-ui.git",
    "publish:react-ui": "yarn --cwd ./build publish --non-interactive",
    "storybook": "start-storybook -p 6060",
    "styleguide": "styleguidist server --config .styleguide/config.js",
    "styleguide:build": "styleguidist build --config .styleguide/config.js && styleguidist build --config .styleguide/prod.config.js",
    "lint:flow": "flow check",
    "lint": "npm-run-all --continue-on-error --parallel lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint ./",
    "lint:tslint": "tslint --project tsconfig.json",
    "lint:stylelint": "stylelint \"./**/*.less\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "create-react-context": "^0.2.2",
    "csstype": "^2.6.1",
    "exenv": "^1.2.2",
    "fbemitter": "^2.1.1",
    "fbjs": "^0.8.17",
    "invariant": "2.2.4",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "normalize-wheel": "^1.0.1",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.2",
    "raf": "^3.4.0",
    "react-focus-lock": "^1.11.2",
    "react-input-mask": "2.0.4",
    "react-transition-group": "^2.4.0",
    "tabbable": "^3.1.1",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@skbkontur/react-icons": "^3.1.5",
    "@skbkontur/typed-css-modules-loader": "^0.1.4",
    "@storybook/addon-actions": "3.4.8",
    "@storybook/addon-knobs": "3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "3.4.8",
    "@storybook/react": "3.4.8",
    "@types/classnames": "^2.2.5",
    "@types/enzyme": "^3.1.12",
    "@types/exenv": "^1.2.0",
    "@types/fbemitter": "^2.0.32",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^23.3.0",
    "@types/lodash.debounce": "^4.0.3",
    "@types/lodash.isequal": "^4.5.3",
    "@types/lodash.throttle": "^4.1.3",
    "@types/prop-types": "15.5.8",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-input-mask": "2.0.1",
    "@types/react-transition-group": "^2.0.11",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__addon-knobs": "^3.3.1",
    "@types/storybook__addon-links": "^3.3.1",
    "@types/storybook__react": "^3.0.8",
    "@types/tabbable": "^3.1.0",
    "@types/uuid": "^3.4.3",
    "@types/warning": "^3.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.23.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.91.0",
    "flow-typed": "^2.5.1",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "fs-readdir-recursive": "^1.1.0",
    "gh-pages": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "23.4.1",
    "jest-teamcity-reporter": "^0.9.0",
    "json-loader": "^0.5.7",
    "less": "3.7.1",
    "less-loader": "^4.1.0",
    "mkdirp": "^0.5.1",
    "mockdate": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "output-file-sync": "^2.0.1",
    "prettier": "^1.13.7",
    "react": "^16.4.2",
    "react-docgen-typescript": "^1.6.2",
    "react-dom": "^16.4.2",
    "react-styleguidist": "^7.3.11",
    "react-test-renderer": "16.3.1",
    "rimraf": "^2.6.2",
    "semver": "^5.6.0",
    "serve": "^9.2.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.9.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-jest": "^23.0.1",
    "ts-loader": "3.5.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0",
    "webpack-watch-files-plugin": "^1.0.2"
  },
  "peerDependencies": {
    "@skbkontur/react-icons": "^3.0.0",
    "react": ">=15 <=16",
    "react-dom": ">=15 <=16"
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json"
      }
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf||svg|ttf|woff|woff2)$": "<rootDir>/testing/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "roots": [
      "components",
      "lib",
      "testing"
    ]
  }
}
