{
  "name": "retail-ui",
  "version": "1.6.8",
  "description": "UI Components",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/retail-ui.git"
  },
  "bugs": {
    "url": "https://github.com/skbkontur/retail-ui/issues"
  },
  "scripts": {
    "prepublishOnly": "yarn clean && node scripts/publish/prepublish && yarn build",
    "clean": "git clean -fdxqe node_modules",
    "build": "tsc -p ./prod.tsconfig.json && node scripts/build",
    "postpublish": "yarn deploy && npm run publish:react-ui",
    "predeploy": "rimraf .styleguide/build && yarn run styleguide:build",
    "deploy": "gh-pages -a -d .styleguide/build -r git@github.com:skbkontur/react-ui.git",
    "publish:react-ui": "npm publish ./build",
    "storybook": "start-storybook -p 6060",
    "storybook:flat": "cross-env STORYBOOK_FLAT_UI=true start-storybook -p 6060",
    "storybook:build": "run-s storybook:build:*",
    "storybook:build:default": "cross-env enableReactTesting=true build-storybook -o .storybook/build/default",
    "storybook:build:flat": "cross-env enableReactTesting=true STORYBOOK_FLAT_UI=true build-storybook -o .storybook/build/flat",
    "storybook:test": "run-p storybook:test:*",
    "storybook:test:default": "cross-env enableReactTesting=true start-storybook --ci -p 6060",
    "storybook:test:flat": "cross-env enableReactTesting=true STORYBOOK_FLAT_UI=true start-storybook --ci -p 6061",
    "storybook:serve": "run-p storybook:serve:*",
    "storybook:serve:default": "serve -l 6060 .storybook/build/default",
    "storybook:serve:flat": "serve -l 6061 .storybook/build/flat",
    "styleguide": "styleguidist server --config .styleguide/config/dev.config.js",
    "styleguide:build": "node .styleguide/build",
    "lint:flow": "flow check",
    "lint": "run-p -c lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint ./",
    "lint:tslint": "tslint --project tsconfig.json",
    "lint:stylelint": "stylelint \"./**/*.less\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "creevey": "wait-on -t 300000 http-get://localhost:6060/ http-get://localhost:6061/ && creevey -c .creevey/config.ts",
    "creevey:ui": "npm run creevey -- --ui"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "classnames": "^2.2.6",
    "create-emotion": "^10.0.14",
    "create-react-context": "^0.3.0",
    "csstype": "2.6.7",
    "eventemitter3": "^4.0.0",
    "invariant": "2.2.4",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "normalize-wheel": "^1.0.1",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react-focus-lock": "^1.11.2",
    "react-input-mask": "2.0.4",
    "react-transition-group": "^2.4.0",
    "shallowequal": "^1.1.0",
    "stylis-plugin-extra-scope": "^0.1.1",
    "tabbable": "^4.0.0",
    "tslib": "^1.10.0",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-object-assign": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/plugin-transform-typeof-symbol": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@skbkontur/react-icons": "^3.2.1",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react": "5.2.8",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/invariant": "^2.2.30",
    "@types/jest": "^24.0.23",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.throttle": "^4.1.6",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-input-mask": "2.0.5",
    "@types/react-transition-group": "^2.0.11",
    "@types/selenium-webdriver": "^4.0.5",
    "@types/shallowequal": "^1.1.1",
    "@types/tabbable": "^3.1.0",
    "@types/uuid": "^3.4.6",
    "@types/warning": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "core-js": "^3.4.8",
    "creevey": "^0.2.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "flow-bin": "^0.113.0",
    "flow-typed": "^2.6.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "fs-extra": "^8.0.1",
    "fs-readdir-recursive": "^1.1.0",
    "gh-pages": "^2.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.9.0",
    "jest-teamcity-reporter": "^0.9.0",
    "less": "3.10.3",
    "less-loader": "^5.0.0",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "output-file-sync": "^2.0.1",
    "prettier": "^1.14.2",
    "react": "^16.12.0",
    "react-docgen-typescript": "^1.16.1",
    "react-dom": "^16.12.0",
    "react-styleguidist": "^7.3.11",
    "rimraf": "^3.0.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "semver": "^6.3.0",
    "serve": "^11.2.0",
    "style-loader": "^1.0.1",
    "stylelint": "^12.0.0",
    "stylelint-config-css-modules": "^2.1.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "6.2.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typed-css-modules-loader": "^0.0.18",
    "typescript": "^3.7.3",
    "url-loader": "^3.0.0",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-watch-files-plugin": "^1.0.3"
  },
  "peerDependencies": {
    "react": ">=15 <=16",
    "react-dom": ">=15 <=16"
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf||svg|ttf|woff|woff2)$": "<rootDir>/components/__mocks__/fileMock.ts",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "__tests__(\\\\|/).*(\\.|-)test\\.(j|t)sx?$",
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ]
  }
}
