    "@types/storybook__react": "^3.0.8",
    "@types/uuid": "^3.4.3",
    "@types/warning": "^3.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
